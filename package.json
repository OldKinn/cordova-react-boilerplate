{
  "name": "cordova-react-boilerplate",
  "version": "1.0.0",
  "description": "搭建cordova应用开发的环境，使用React、React-router、Redux",
  "scripts": {
    "clean": "rimraf ./builder/dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config ./builder/webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node ./builder/devServer.js",
    "app": "gulp copy && cordova run android",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OldKinn/cordova-react-boilerplate.git"
  },
  "keywords": [
    "cordova",
    "react",
    "reactjs",
    "boilerplate",
    "webpack",
    "edit",
    "babel",
    "react-transform"
  ],
  "author": "Leon <leonooo@163.com> (https://github.com/OldKinn)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/OldKinn/cordova-react-boilerplate/issues"
  },
  "homepage": "https://github.com/OldKinn/cordova-react-boilerplate",
  "devDependencies": {
    "async": "^2.0.1",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chalk": "^1.1.3",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-uglify": "^1.5.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "lodash": "^4.14.0",
    "rc-queue-anim": "^0.12.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.5.2"
  }
}
